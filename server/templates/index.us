<% _.each(templates, function(template) { %>
<script type="text/template" id="<%= template.name %>">
    <%= template.content %>
</script>
<% }); %>

<% _.each(jsList, function(script) { %>
<script type="text/javascript" src="/js/<%= script %>"></script>
<% }); %>

</body>
</html>
